IFS=.
set $0
IFS=

while [ "$1" != "trb" ]
do
	shift
	if  [ $# -lt 2 ]
	then
		echo 
		echo "trb setup script named illegally"
		exit
	fi
done
shift
unit=$1

echo -n " trb$unit"
/usr/sbin/trbsetup trb$unit
