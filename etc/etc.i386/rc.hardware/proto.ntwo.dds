#
# ntwo setup script to configure csu on 56k dds board
unit=
IFS=.
for i in $0
do
    unit=$i
done
IFS=

echo -n " ntwo$unit"
ifconfig ntwo$unit down
n2setup -u $unit <<EOF
#
# uncomment following to make driver silent about receive
# errors such as parity and framing
#
#receive data errors = silent;		
EOF
