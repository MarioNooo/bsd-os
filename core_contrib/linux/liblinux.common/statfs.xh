/*
 * Transforms for statfs
 */

%{
#include <sys/param.h>
#include <sys/mount.h>
%}

struct statfs {
	int		f_type;
	int		f_bsize;
	int		f_blocks;
	int		f_bfree;
	int		f_bavail;
	int		f_files;
	int		f_ffree;
	long long	linux_f_fsid;
	int		linux_f_namelen;
	int		linux_f_spare[6];

	out (n, f, len) {
		f->linux_f_namelen = 255;
		f->linux_f_fsid = *(long long *)&n->f_fsid;
	}
};
