flag int map_t {
	MAP_SHARED		0x01;
	MAP_PRIVATE		0x02;
	MAP_FIXED		0x10;
	MAP_ANON		0x20;
	LINUX_MAP_GROWSDOWN	0x0100;
	LINUX_MAP_DENYWRITE	0x0800;
	LINUX_MAP_EXECUTABLE	0x1000;
	LINUX_MAP_LOCKED	0x2000;
};
