.PHONY: all clean ci install realclean mostlyclean distclean

EXEC = LPRng.pm accounting.pl fixid fixupdate lpq_in_perl lpr_in_perl lprm_in_perl make_lpd_conf make_printcap_use read_conf remote_active test_read

all:
	chmod a+x $(EXEC)

clean install realclean mostlyclean distclean:
	echo done

CI=
CO=-kv

cifast ci:
	checkin() { \
		rcs -l $$1; \
		ci $(CI) -mUpdate -t-Initial $$1; \
		co $(CO) $$1; \
	}; \
	for i in *; do \
		echo $$i ; \
		if [ -f "$$i" ] ; then \
			checkin $$i; \
		fi; \
	done;
